<div class="container">
  <header>
    <h1 id="paper-title">NodeNewScraper Daily</h1>
  </header>
  <main>
    <button id="scrapeBtn">Scrape Dat</button>
    <button id="displayBtn">Show Dem Results</button>
    <div>
      {{#each data}}
      <div>
        <a href="{{ link }}">{{ title }}</a>
        <p>{{ summary }}</p>
        <p>{{ note }}</p>
      </div>
      <div>
        <p>Da Notes</p>
        <input type="text" id="{{ _id }}">
        <button data-id="{{ _id }}" class="addNote">Add a note</button>
      </div>
      {{/each}}
    </div>
  </main>
  <footer>Footer</footer>
</div>





{{!-- jQuery --}}
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script>
{{!-- JavaScript --}}
<script src="assets/js/app.js"></script>